# Roof Scanner Plugin Environment Configuration
# Copy this file to .env and fill in your actual values

# Google Maps API Key (required for address search - used at build time)
VITE_GOOGLE_MAPS_API_KEY=your_google_maps_api_key_here

# Default Solar Server Configuration  
VITE_SOLAR_API_URL=http://localhost:3000
VITE_DEFAULT_SOLAR_API_KEY=your_solar_api_key_here

# Development Settings
NODE_ENV=development

# Note: The Google Maps API key is built into the widget at build time.
# Customers embedding the widget don't need to provide their own API key.